    "zocci": "0.2.4",
    "zygon": "0.1.0"
  },
  "devDependencies": {
    "jshint": "^2.9.1",
    "mocha": "^2.4.5",
    "istanbul": "^0.4.2"
  },
  "engines": {
    "node": ">=0.12"
  },
  "preferGlobal": true,
  "bin": {
